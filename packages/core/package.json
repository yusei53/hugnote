{
  "name": "@pichikoto/core",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./domain/*": {
      "types": "./src/domain/*/index.ts",
      "default": "./src/domain/*/index.ts"
    },
    "./utils": {
      "types": "./src/utils/index.ts",
      "default": "./src/utils/index.ts"
    },
    "./utils/*": {
      "types": "./src/utils/*.ts",
      "default": "./src/utils/*.ts"
    }
  },
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "test": "vitest",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/uuid": "^10.0.0",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4",
    "@types/node": "^24.3.0"
  }
}
